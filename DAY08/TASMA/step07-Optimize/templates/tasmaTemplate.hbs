<li class="task clearfix">
	<div class="task-desc">
		<input class="complete" type="checkbox" {{#if checked}}checked="checked"{{/if}}>
		<input class="description" maxlength="12" type="text" placeholder="등록할 내용을 기입해주세요." value="{{description}}">
	</div>
	<button type="button" class="anim button remove">제거</button>
</li>